@model List<PMS.ViewModels.HomeViewModel>
@using PMS.Helpers

@{
    ViewBag.Title = "Resume";
}

<div class="section">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                @*HEADER*@
                <h2>@Model.Where(w => w.TypeID == (int)PMSUtilities.MasterType.Header).Select(s => s.Description).FirstOrDefault()</h2>
                <p>&nbsp;</p>

                @*BODY*@
                <div class="embed-responsive embed-responsive-4by3">
                    <iframe src="@Url.Content(Model.Where(w => w.TypeID == (int)PMSUtilities.MasterType.Body).Select(s => s.Description).FirstOrDefault())" width="100%" height="1000px" frameborder="0"></iframe>
                    @*<iframe src="Content/media/sample.pdf" width="100%" height="1000px" frameborder="0"></iframe>*@
                </div>

                <!-- <div class="embed-responsive embed-responsive-4by3">
                    <embed src="media/sample.pdf" width="100%" height="1000px" />
                </div> -->
                <p>&nbsp;</p>
            </div>
        </div>
    </div>
</div>
@section Scripts
{
    <script>
        setActiveMenu('liResume');
    </script>
}