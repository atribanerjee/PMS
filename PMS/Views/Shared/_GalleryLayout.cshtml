            <a href="#" class="cd-top">Top</a>
            <footer>
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <p class="text-center text-capitalize">
                            Copyright &copy;
                            <a href="https://www.kelseycarlisle.com/"
                               target="_blank">KelseyCarlisle</a> 2020-@DateTime.Now.AddYears(1).Year.
                        </p>
