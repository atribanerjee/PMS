                    <li>@Html.ActionLink("Resume", "Index", "Resume", new { }, new { @class = "", @id = "liResume" })</li>
                    <li>@Html.ActionLink("Reel", "Index", "Reel", new { }, new { @class = "", @id = "liReel" })</li>
                    <li>@Html.ActionLink("Gallery", "Index", "Gallery", new { }, new { @class = "active", @id = "liGallery" })</li>
